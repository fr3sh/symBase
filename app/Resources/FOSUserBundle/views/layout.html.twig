{% extends '::base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
   
    {% stylesheets 'bundles/app/style/*' filter='cssrewrite' %}
       <link href="{{ asset('bundles/app/style/FOSstyle.css') }}" rel="stylesheet">
    {% endstylesheets %}
{% endblock %}


     {% block javascripts %}
            {{ parent() }}
                 <script type="text/javascript" src="{{ asset('bundles/app/js/passwordMeater.js') }}"></script>
     {% endblock %}


{% block body %}
   



        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                       {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}

        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>

{% endblock %}