<!DOCTYPE html>
<html lang="pl">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
        <meta name="description" content="">
        <meta name="author" content="Fr3sh">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        <title>{% block title %}Welcome!{% endblock %}</title>
 
        {% block stylesheets %}
            {% stylesheets 'bundles/app/style/*' filter='cssrewrite' %}
            <!-- Latest compiled and minified CSS -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" integrity="sha512-dTfge/zgoMYpP7QbHy4gWMEGsbsdZeCXz7irItjcC3sPUFtf0kuFbDz/ixG7ArTxmDjLXDmezHubeNikyKGVyQ==" crossorigin="anonymous">
            <link href="{{ asset('bundles/app/style/main.css') }}" rel="stylesheet">
            <link href="{{ asset('bundles/app/style/custom.css') }}" rel="stylesheet">
            <link rel="stylesheet" href="https://cdn.datatables.net/1.10.10/css/dataTables.bootstrap.min.css">
            <link rel="stylesheet" href="{{ asset('bundles/app/other/font-awesome-4.4.0/css/font-awesome.min.css') }}">

            <link href="{{ asset('bundles/app/style/FOSstyle.css') }}" rel="stylesheet">
            {% endstylesheets  %}

        {% endblock %}



        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->



        {% block javascripts %}
            <script src="../../assets/js/ie-emulation-modes-warning.js"></script>
            <!-- Bootstrap core JavaScript ================================================== -->
            <!-- Placed at the end of the document so the pages load faster -->
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
            <!-- Latest compiled and minified JavaScript -->


            <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
            <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
            <script src="//cdn.datatables.net/1.10.10/js/jquery.dataTables.min.js"></script>
            <script src="https://cdn.datatables.net/1.10.10/js/dataTables.bootstrap.min.js" ></script>
            <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
            {# skryp wyswietlajacy animacje ładowania    #}
            <script src="https://cdn.datatables.net/buttons/1.1.0/js/dataTables.buttons.min.js"></script>  

            <script type="text/javascript" src="{{ asset('bundles/app/other/pdfmake-master/build/pdfmake.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/other/pdfmake-master/build/vfs_fonts.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/js/AutotablePDFmake.js') }}"></script>

            <script src="//cdn.datatables.net/buttons/1.1.0/js/buttons.html5.min.js"></script>  

            <script src="{{ asset('js/Jszip/dist/jszip.js') }}"></script> 

            <script src="{{ asset('bundles/app/js/spin.js') }}" ></script>
            <script src="{{ asset('bundles/app/js/jsForAll.js') }}" ></script>
            <script src="{{ asset('bundles/app/js/printme/jquery-printme.js') }}" ></script>




            <!-- Skrypty niezbedne do EXPORTU TABEL PDF ITD  -->
            <script type="text/javascript" src="{{ asset('bundles/app/other/tableExport.jquery.plugin-master2/libs/FileSaver/FileSaver.min.js') }}"></script>
            {#  Brak polskich znaków dyskfalifikacja
            <script type="text/javascript" src="{{ asset('other/tableExport.jquery.plugin-master2/libs/jsPDF/jspdf.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('other/tableExport.jquery.plugin-master2/libs/jsPDF-AutoTable/jspdf.plugin.autotable.js') }}"></script>#}
            <script type="text/javascript" src="{{ asset('bundles/app/other/tableExport.jquery.plugin-master2/libs/html2canvas/html2canvas.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/other/tableExport.jquery.plugin-master2/tableExport.min.js') }}"></script>






            <!-- Skrypty niezbedne do EXPORTU TABEL PDF ITD  -->

        {% endblock %}


    </head>

    <body>

        <nav class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{path('homepage')}}">Fr3sh Design</a>
                </div>
                <div id="navbar" class="collapse navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="{{path('homepage')}}">Strona Główna</a></li>
                        <li><a href="#about">O nas</a></li>
                        <li><a href="#contact">Kontakt</a></li>
                    </ul>
                    <ul class="nav navbar-right navbar-nav">


                        {# <li>{#<a href="#about">Login</a>#}

                        {% if is_granted('ROLE_PREVIOUS_ADMIN') %}
                            <li>  <a href="{{ path('homepage', {'_switch_user': '_exit'}) }}">Exit impersonation</a></li>
                            {% endif %}


                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}

                            <li>   <div>     {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} </div> </li>
                            <li>         <a href="{{ path('fos_user_security_logout') }}"> {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}  </a></li>

                            <li><a href="">Admin</a></li>
                            {% else %}

                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><b>Zaloguj</b> <span class="caret"></span></a>
                                <ul id="login-dp" class="dropdown-menu">
                                    <li>
                                        <div class="row">
                                            <div class="col-md-12">
                                                Login via
                                                <div class="social-buttons">
                                                    <a href="#" class="btn btn-fb"><i class="fa fa-facebook"></i> Facebook</a>
                                                    <a href="#" class="btn btn-tw"><i class="fa fa-twitter"></i> Twitter</a>
                                                </div>
                                                or

                                                <form class="form" role="form" method="post" action="{{ path("fos_user_security_check") }}" accept-charset="UTF-8" id="login-nav">
                                                    <input type="hidden" name="_csrf_token" value="{{  csrf_token|default('') }}" />

                                                    <div class="form-group">
                                                        <label class="sr-only" for="username">{{ 'security.login.username'|trans({}, 'FOSUserBundle') }}</label>

                                                        <input type="text" id="username" name="_username" class="form-control"  placeholder="Email address" required>
                                                    </div>
                                                    <div class="form-group">

                                                        <label class="sr-only" for="password">{{ 'security.login.password'|trans({}, 'FOSUserBundle') }}</label>
                                                        <input type="password" id="password" name="_password" class="form-control"  placeholder="Password" required>
                                                        <div class="help-block text-right"><a href="">Forget the password ?</a></div>
                                                    </div>
                                                    <div class="form-group">

                                                        <input type="submit" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans({}, 'FOSUserBundle') }}" class="btn btn-primary btn-block"></input>
                                                    </div>

                                                    <div class="">
                                                        <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
                                                        <label for="remember_me">{{ 'security.login.remember_me'|trans({}, 'FOSUserBundle') }}</label>
                                                    </div>

                                                </form>
                                            </div>
                                            <div class="bottom text-center">
                                                New here ? <a href="#"><b>Join Us</b></a>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </li>



                            <li>        <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a></li>
                            <li><a href="#about">Rejsetracja</a></li>
                            {% endif %}

                        {# </li>#}

                    </ul>

                </div><!--/.nav-collapse -->
            </div>
        </nav>

        <div class="container containerTOP">

       

            {% block body %}


            {% endblock %}  
        </div><!-- /.container -->



    </body>
</html>
